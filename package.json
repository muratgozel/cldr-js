{
  "name": "locale-util",
  "version": "3.0.1",
  "description": "Locale data generators written in node.js. Uses reliable sources. Generated data available as JS/JSON objects.",
  "main": "build/index.js",
  "type": "module",
  "types": "types",
  "imports": {
    "#src/*": "./build/*.js"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "precompile": "rm -rf build/*",
    "compile": "tsc -p tsconfig.json",
    "generate": "node build/generateResources.js",
    "build": "npm run compile && node esbuild.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/locale-util.git"
  },
  "keywords": [
    "cldr-json",
    "cldr",
    "locale-gen",
    "locale",
    "i18n",
    "localization",
    "phone-number",
    "country-codes",
    "languages",
    "timezones",
    "currency-codes"
  ],
  "author": {
    "name": "Murat GÃ¶zel",
    "email": "murat@gozel.com.tr",
    "url": "https://gozel.com.tr"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/muratgozel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/locale-util/issues"
  },
  "homepage": "https://github.com/muratgozel/locale-util#readme",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "fast-xml-parser": "^4.1.3",
    "jest": "^29.5.0",
    "moment-timezone": "^0.5.41",
    "node-releaser": "^2.0.0-beta.4",
    "typescript": "^4.9.5"
  }
}