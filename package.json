{
  "name": "locale-util",
  "version": "3.2.0",
  "description": "Locale data generators written in node.js. Uses reliable sources. Generated data available as JS/JSON objects.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./build/index.js",
      "require": "./dist/locale-util.js"
    }
  },
  "types": "./types/index.d.ts",
  "main": "./dist/locale-util.js",
  "module": "./build/index.js",
  "scripts": {
    "presetup": "rm -rf src/data/*",
    "setup": "node setup/generateResources.js",
    "precompile": "rm -rf build/*",
    "compile": "tsc -p tsconfig.json",
    "build": "rollup --config rollup.config.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "docs": "typedoc --includeVersion --excludePrivate src/index.ts types/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/locale-util.git"
  },
  "keywords": [
    "cldr-json",
    "cldr",
    "locale-gen",
    "locale",
    "i18n",
    "localization",
    "phone-number",
    "country-codes",
    "languages",
    "timezones",
    "currency-codes"
  ],
  "author": {
    "name": "Murat GÃ¶zel",
    "email": "murat@gozel.com.tr",
    "url": "https://gozel.com.tr"
  },
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/muratgozel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/locale-util/issues"
  },
  "homepage": "https://github.com/muratgozel/locale-util#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "core-js": "^3.31.1",
    "eslint": "^8.35.0",
    "fast-xml-parser": "^4.1.3",
    "jest": "^29.5.0",
    "moment-timezone": "^0.5.41",
    "node-releaser": "^2.0.0-beta.4",
    "rollup": "^4.9.2",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "@babel/runtime-corejs3": "^7.22.6",
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/strictest": "^2.0.0"
  }
}